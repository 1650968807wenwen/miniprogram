<!--index.wxml-->
<van-tabbar active="{{ active }}" bind:change="onChange" active-color="#000000">
  <van-tabbar-item wx:for="{{ list }}" wx:key="index">
    <image slot="icon" src="{{ item.icon }}" mode="aspectFit" style="width: 30px; height: 18px;" />
    <image slot="icon-active" src="{{ item.icon_active }}" mode="aspectFit" style="width: 30px; height: 18px;" />
    {{ item.text }}
  </van-tabbar-item>
</van-tabbar>